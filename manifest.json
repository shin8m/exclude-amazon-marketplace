{
    "name": "Exclude Amazon Marketplace",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Exclude Amazon Marketplace.",
    "browser_action": {
        "default_popup": "./popup.html"
    },
    "content_scripts": [{
        "matches": ["https://www.amazon.co.jp/s?k=*"],
        "js": [
            "content.js"
        ]
    }],
    "permissions": [
        "storage"
    ]
}
